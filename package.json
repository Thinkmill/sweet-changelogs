{
  "name": "sweet-changelogs",
  "version": "1.0.0",
  "description": "A sweet little changelog generator that works off git tags",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma es5 --use-tabs --write"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "author": "Ben Conolly",
  "bin": "./index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "dotenv": "^4.0.0",
    "graphql": "^0.11.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.0",
    "eslint-config-keystone": "^3.0.0",
    "eslint-config-prettier": "^2.5.0",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  }
}
